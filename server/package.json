{
   "name": "server",
   "version": "1.0.0",
   "main": "index.js",
   "license": "MIT",
   "dependencies": {
      "cors": "^2.8.5",
      "debug": "^4.3.2",
      "dotenv": "^10.0.0",
      "express": "^4.17.1",
      "morgan": "^1.10.0"
   },
   "devDependencies": {
      "@types/cors": "^2.8.12",
      "@types/debug": "^4.1.7",
      "@types/express": "^4.17.13",
      "@types/morgan": "^1.9.3",
      "@types/node": "^16.10.3",
      "nodemon": "^2.0.13",
      "ts-node": "^10.2.1",
      "typescript": "^4.4.3"
   },
   "scripts": {
      "start": "nodemon ./src/index.ts"
   },
   "nodemonConfig": {
      "env": {
         "NODE_ENV": "development",
         "DEBUG": "server"
      },
      "execMap": {
         "ts": "ts-node"
      },
      "watch": [
         "./src"
      ],
      "ext": ".js,.ts"
   }
}

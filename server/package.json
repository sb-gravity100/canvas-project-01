{
   "name": "server",
   "version": "1.0.0",
   "main": "index.js",
   "license": "MIT",
   "dependencies": {
      "cors": "^2.8.5",
      "debug": "^4.3.2",
      "express": "^4.17.1",
      "express-handlebars": "^5.3.4",
      "express-jwt": "^6.1.0",
      "morgan": "^1.10.0",
      "socket.io": "^4.2.0"
   },
   "devDependencies": {
      "@types/cors": "^2.8.12",
      "@types/debug": "^4.1.7",
      "@types/express": "^4.17.13",
      "@types/express-handlebars": "^5.3.1",
      "@types/express-jwt": "^6.0.2",
      "@types/morgan": "^1.9.3",
      "@types/node": "^16.10.4",
      "nodemon": "^3.1.10",
      "ts-node": "^10.9.2",
      "typescript": "^5.8.3"
   },
   "scripts": {
      "start": "nodemon ./src/index.ts"
   },
   "nodemonConfig": {
      "env": {
         "NODE_ENV": "development",
         "DEBUG": "server"
      },
      "execMap": {
         "ts": "ts-node"
      },
      "watch": [
         "./src"
      ],
      "ext": ".js,.ts"
   }
}
